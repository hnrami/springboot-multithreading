      query: {
        bool: {
          must: [
            {
              constant_score: {
                filter: {
                  terms: {
                    recorddeleteddtim: [
                      "NULL"
                    ]
                  }
                }
              }
            },
            {
              bool: {
                should: [
                  {
                    constant_score: {
                      filter: {
                        match_phrase_prefix: {
                          "recordsource.text": {
                            query: "usa",
                            slop: 0,
                            max_expansions: 75,
                            zero_terms_query: "NONE"
                          }
                        }
                      }
                    }
                  },
                  {
                    constant_score: {
                      filter: {
                        match: {
                          "recordsource.text": {
                            query: "usa",
                            operator: "OR",
                            prefix_length: 0,
                            max_expansions: 75,
                            fuzzy_transpositions: true,
                            lenient: false,
                            zero_terms_query: "NONE",
                            auto_generate_synonyms_phrase_query: true
                          }
                        }
                      },
                      boost: 10.0
                    }
                  },
                  {
                    constant_score: {
                      filter: {
                        prefix: {
                          "recordsource.text": {
                            value: "usa"
                          }
                        }
                      },
                      boost: 5.0
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      _source: false,
      aggregations: {
        recordsource: {
          terms: {
